{
	"name": "rater",
	"version": "3.2.1",
	"description": "Wikipedia userscript that helps assess pages for WikiProjects",
	"keywords": [
		"wikipedia",
		"wikiproject",
		"userscript",
		"assessment",
		"quality",
		"importance",
		"mediawiki"
	],
	"homepage": "https://github.com/evad37/rater",
	"bugs": {
		"url": "https://github.com/evad37/rater/issues"
	},
	"type": "module",
	"browser": "index.js",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"lint:eslint": "eslint index.js \"src/**\"",
		"lint:stylelint": "stylelint \"src/**/*.css\"",
		"lint:fix": "eslint index.js \"src/**\" --fix && stylelint \"src/**/*.css\" --fix",
		"lint": "npm run lint:eslint && npm run lint:stylelint",
		"build:dev": "vite build --mode development",
		"build:prod": "vite build --mode production",
		"build": "npm run build:dev && npm run build:prod",
		"prebuild": "npm run lint",
		"prepublishOnly": "npm run build"
	},
	"author": {
		"name": "Evad37",
		"url": "https://en.wikipedia.org/wiki/User:Evad37"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/evad37/rater.git",
		"directory": "."
	},
	"license": "(MIT OR CC-BY-4.0)",
	"devDependencies": {
		"@eslint/json": "^0.13.2",
		"@eslint/markdown": "^7.3.0",
		"@stylistic/eslint-plugin": "^5.4.0",
		"@vitest/coverage-v8": "^2.1.9",
		"eslint": "^9.35.0",
		"eslint-plugin-es-x": "^9.1.0",
		"eslint-plugin-jsdoc": "^57.0.8",
		"eslint-plugin-jsonc": "^2.21.0",
		"eslint-plugin-security": "^3.0.1",
		"eslint-plugin-unicorn": "^61.0.2",
		"jquery": "^3.7.1",
		"jsdom": "^25.0.1",
		"stylelint": "^16.25.0",
		"stylelint-config-standard": "^39.0.0",
		"vite": "^7.1.9",
		"vite-plugin-css-injected-by-js": "^3.5.2",
		"vitest": "^2.1.9"
	}
}
